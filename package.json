{
  "name": "cougarcs-adminportal",
  "version": "0.0.1",
  "description": "admin portal for cougarcs",
  "main": "server/index.js",
  "scripts": {
    "start": "cd client && npm run build && cd .. && nodemon server/index.ts",
    "lint": "npx prettier --write ./server/** && eslint \"./server/**\" && npx prettier --write ./logger/** && eslint \"./logger/**\"",
    "prepare": "npx husky install"
  },
  "keywords": [
    "cougarcs",
    "admin",
    "portal",
    "member"
  ],
  "author": "Adil Iqbal",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.990.0",
    "create-react-app": "^4.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^3.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "server/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "logger/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
